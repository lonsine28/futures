<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

	<table name="usertest" catalog="futures" class="User">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="f_admin" catalog="hundsun_future" class="Admin">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="f_column" catalog="hundsun_future" class="Column">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="f_feedback_tpye" catalog="hundsun_future"
		class="FeedbackType">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="f_news" catalog="hundsun_future" class="News">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="f_problem" catalog="hundsun_future" class="Problem">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="f_reply" catalog="hundsun_future" class="Reply">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="f_user" catalog="hundsun_future" class="User">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>